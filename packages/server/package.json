  "devDependencies": {
    "@types/cors": "2.8.8",
    "@types/express": "4.17.8",
    "@types/node": "14.11.8",
    "ts-node": "9.0.0",
    "ts-node-dev": "^1.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  }
}
