    "@reduxjs/toolkit": "2.2.7",
    "@sentry/react": "8.38.0",
    "autoprefixer": "latest",
    "bootstrap": "5.3.3",
    "classnames": "2.2.6",
    "cssnano": "4.1.10",
    "debounce": "1.2.0",
