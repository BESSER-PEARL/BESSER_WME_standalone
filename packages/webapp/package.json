    "react-dom": "16.13.1",
    "react-redux": "7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "rxjs": "6.6.3",
    "shared": "1.0.0",
    "styled-components": "5.2.1",
    "tslib": "2.0.3",
    "typesafe-actions": "5.1.0",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@types/react": "16.9.55",
    "@types/react-bootstrap": "1.0.1",
    "@types/react-dom": "16.9.9",
